    {
      "parameters": {
        "jsCode": "// Collect all results and count for sync log\n// This node receives items from both paths: with transcript and without\nconst items = $input.all();\n\nlet synced = 0;\nlet withTranscript = 0;\nlet withoutTranscript = 0;\n\nfor (const item of items) {\n  // Check if this item went through the transcript path\n  if (item.json.hasTranscript === true || item.json.full_content) {\n    withTranscript++;\n  } else {\n    withoutTranscript++;\n  }\n  synced++;\n}\n\nreturn [{\n  json: {\n    itemsProcessed: synced,\n    itemsSynced: synced,\n    withTranscript,\n    withoutTranscript,\n    status: 'completed'\n  }\n}];"
      },
