    {
      "parameters": {
        "jsCode": "// Get YouTube videos and filter to only NEW videos\nconst videos = $input.all();\n\n// Get existing IDs from workflow static data\nconst existingIds = new Set($getWorkflowStaticData('global').existingYouTubeIds || []);\n\n// Handle case where no videos from YouTube\nif (!videos || videos.length === 0) {\n  return [{\n    json: {\n      _noNewVideos: true,\n      itemsProcessed: 0\n    }\n  }];\n}\n\nconst timestamp = new Date().toISOString();\n\n// Format and filter to only NEW videos\nconst newVideos = videos\n  .map(item => {\n    const video = item.json;\n    const snippet = video.snippet || {};\n    const contentDetails = video.contentDetails || {};\n    const resourceId = snippet.resourceId || {};\n    \n    // Get video ID from multiple possible locations\n    const videoId = contentDetails.videoId || resourceId.videoId || '';\n    \n    return {\n      videoId,\n      title: snippet.title || 'Untitled Video',\n      content_type: 'youtube',\n      url: videoId ? `https://www.youtube.com/watch?v=${videoId}` : '',\n      source_platform: 'youtube',\n      source_id: videoId,\n      status: 'published',\n      author: snippet.channelTitle || 'GrowthLab',\n      description: (snippet.description || '').substring(0, 500),\n      full_content: snippet.description || '',\n      date_created: snippet.publishedAt || timestamp,\n      synced_at: timestamp\n    };\n  })\n  .filter(v => v.source_id && !existingIds.has(v.source_id)); // Only NEW videos\n\n// If no new videos, return marker\nif (newVideos.length === 0) {\n  return [{\n    json: {\n      _noNewVideos: true,\n      itemsProcessed: 0,\n      existingCount: existingIds.size,\n      totalFromYouTube: videos.length\n    }\n  }];\n}\n\n// Return each new video as separate item for processing\nreturn newVideos.map(video => ({ json: video }));"
      },
